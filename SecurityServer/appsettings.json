{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=usersdatabase2.sqlite"
  },
  "StsConfig": {
    "StsServerIdentityUrl": "https://localhost:44318",
    "ClientUrl": "https://localhost:4200"
  },
  "UseLocalCertStore": "false",
  "CertificateThumbprint": "",
  "CertificateNameKeyVault": "",
  "AzureKeyVaultEndpoint": "",
  "EmailSettings": {
    "SendGridApiKey": "TODO_EMAIL_API_KEY",
    "SenderEmailAddress": "TODO_EMAIL_ADDRESS"
  },
  "fido2": {
    "serverDomain": "localhost",
    "serverName": "StsFabian",
    "origin": "https://localhost:44318",
    "timestampDriftTolerance": 300000
  },
  "fido2mds": {
    "MDSAccessKey": null
  },
  "ClientAppSettings": {
    "stsServer": "https://offeringsolutions-sts.azurewebsites.net",
    "redirect_url": "https://localhost:4200",
    "client_id": "talerClient",
    "response_type": "code",
    "scope": "taler_api openid profile",
    "post_logout_redirect_uri": "https://localhost:4200",
    "start_checksession": true,
    "silent_renew": true,
    "startup_route": "/home",
    "forbidden_route": "/forbidden",
    "unauthorized_route": "/unauthorized",
    "log_console_warning_active": true,
    "log_console_debug_active": true,
    "max_id_token_iat_offset_allowed_in_seconds": 10,
    "apiServer": "https://offeringsolutions-sts.azurewebsites.net"
  }
